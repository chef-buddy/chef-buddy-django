container_commands::
  01_migratemyapp:
    command: "python manage.py migrate --noinput"
    leader_only: true
  02_collectstatic:
    command: "python manage.py collectstatic --noinput"
    leader_only: true

option_settings:
  - option_name: Timeout
    value: 1000
  - option_name: DJANGO_SETTINGS_MODULE
    value: chef_buddy_django.settings.prod
